{
  "name": "Tab Refresher",
  "manifest_version": 2,
  "description": "This extension will refresh browser tabs at random intervals",
  "version": "0.0.10",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "lib/img/icon48.png",
    "default_popup": "lib/popup-timer/popup-timer.html",
    "default_title": "Auto Tab Refresh"
  },
  "icons": {
    "16": "lib/img/icon16.png",
    "48": "lib/img/icon48.png",
    "128": "lib/img/icon128.png"
  },
  "background": {
    "scripts": [
      "lib/vendor/babel-polyfill.js",
      "lib/vendor/chrome-promise.js",
      "lib/node_modules/jquery/dist/jquery.js",
      "lib/utils/utils.js",
      "lib/utils/storage-manager.js",
      "lib/utils/tab-manager.js",
      "lib/events/alarms.js"
    ],
    "persistent": false
  }
}
