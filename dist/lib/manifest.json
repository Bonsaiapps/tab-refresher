{
  "name": "tab-refresher",
  "manifest_version": 2,
  "description": "Refresh intervals on chrome tabs",
  "version": "0.0.14",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "<all_urls>"
  ],
  "icons": {
    "16": "$assets/icon16.png",
    "48": "$assets/icon48.png",
    "128": "$assets/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "Auto Tab Refresh"
  },
  "background": {
    "scripts": [
      "events/index.js"
    ],
    "persistent": false
  }
}