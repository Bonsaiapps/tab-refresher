{
  "name": "Tab Refresher",
  "manifest_version": 2,
  "description": "This extension will refresh browser tabs at random intervals",
  "version": "0.0.10",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "Auto Tab Refresh"
  },
  "background": {
    "scripts": [
      "events/index.js"
    ],
    "persistent": false
  }
}
