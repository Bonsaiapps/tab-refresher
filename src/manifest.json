{
  "name": "Tab Refresher",
  "manifest_version": 2,
  "description": "This extension will refresh browser tabs at random intervals",
  "version": "0.0.10",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup/popup.html",
    "default_title": "Auto Tab Refresh"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "vendor/babel-polyfill.js",
      "vendor/chrome-promise.js",
      "node_modules/jquery/dist/jquery.js",
      "utils/utils.js",
      "utils/storage-manager.js",
      "utils/tab-manager.js",
      "events/alarms.js"
    ],
    "persistent": false
  }
}
